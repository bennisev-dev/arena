    "lucide-react": "^0.474.0",
    "next": "^14.2.35",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.17.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.16.13",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.35",
    "postcss": "^8.4.47",
